{
  "name": "vekit-react-template",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build --mode=development",
    "build:testnet": "tsc && vite build --mode=production",
    "build:mainnet": "tsc && vite build --mode=production",
    "clean": "rm -rf dist .turbo node_modules",
    "dev": "vite",
    "dev:testnet": "vite",
    "dev:mainnet": "vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview --mode=development",
    "test": "vitest",
    "test:unit": "vitest run",
    "ts-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx}\"",
    "knip": "knip --config ./knip.json",
    "knip:fix": "knip --config ./knip.json --fix",
    "translate": "dotenv -e .env npx translo-cli@latest",
    "translate:complete": "dotenv -e .env npx translo-cli@latest complete",
    "prepare": "husky"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@tanstack/query-sync-storage-persister": "^5.28.12",
    "@tanstack/react-query": "^5.64.2",
    "@tanstack/react-query-persist-client": "^5.67.3",
    "@vechain/sdk-network": "^2.0.7",
    "@vechain/vechain-contract-types": "^1.5.0",
    "@vechain/vechain-kit": "2.4.1",
    "@vercel/analytics": "^1.5.0",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.3",
    "ethers": "^6.9.0",
    "framer-motion": "^11.2.12",
    "i18next": "^23.10.1",
    "mixpanel-browser": "^2.67.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-hook-form": "^7.52.0",
    "react-i18next": "^15.2.0",
    "react-icons": "^5.2.1",
    "react-router-dom": "^7.6.0",
    "recharts": "^2.15.3",
    "vite": "^5.0.12",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@types/mixpanel-browser": "^2.66.0",
    "@types/node": "^20.17.10",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.53.0",
    "eslint-plugin-i18next": "^6.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "knip": "^5.36.0",
    "prettier": "^3.6.1",
    "typescript": "^5.2.2",
    "vite-plugin-node-polyfills": "^0.17.0",
    "vitest": "^1.2.2"
  },
  "homepage": "https://vechain.github.io/vekit-react-template"
}
